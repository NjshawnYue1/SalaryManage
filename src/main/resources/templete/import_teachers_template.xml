<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet name="emp">
        <section startRow="0" endRow="0" />
        <loop startRow="1" endRow="1" items="emp" var="bean" varType="com.usts.salarymanage.model.Employee">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">bean.empName</mapping>
                <mapping row="1" col="1">bean.empSex</mapping>
                <mapping row="1" col="2">bean.empAge</mapping>
                <mapping row="1" col="3">bean.empDepartment</mapping>
                <mapping row="1" col="4">bean.empCareer</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>